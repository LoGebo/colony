{
  "draftId": "b2241c31-9631-4870-90cc-b4c6c3405009",
  "title": "Lumina - Vehicle Management",
  "htmlContent": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Lumina - Vehicle Management</title>\n    <meta name=\"view-transition\" content=\"same-origin\">\n    <link rel=\"prefetch\" href=\"https://p.superdesign.dev/draft/c6843c32-7c38-41c2-82c2-dc47929810cc\" as=\"document\">\n    <link rel=\"prefetch\" href=\"https://p.superdesign.dev/draft/b2241c31-9631-4870-90cc-b4c6c3405009\" as=\"document\">\n    <link rel=\"prefetch\" href=\"https://p.superdesign.dev/draft/299b7c74-6739-4874-8ae6-0e248e209b1a\" as=\"document\">\n    <link rel=\"prefetch\" href=\"https://p.superdesign.dev/draft/fd7e0571-00be-4a63-90af-22128a834134\" as=\"document\">\n    <link rel=\"prefetch\" href=\"https://p.superdesign.dev/draft/bfe82a67-f200-428c-8db4-04f3c670ce38\" as=\"document\">\n    <link rel=\"prefetch\" href=\"https://p.superdesign.dev/draft/cf51ff25-7e63-4b29-84c8-1889866485e1\" as=\"document\">\n    <link rel=\"prefetch\" href=\"https://p.superdesign.dev/draft/67d9e19b-2841-497d-aab9-948d660f67d2\" as=\"document\">\n    <link rel=\"prefetch\" href=\"https://p.superdesign.dev/draft/766d1f6d-24ec-41fc-b2ec-19418a5bac78\" as=\"document\">\n    <link rel=\"prefetch\" href=\"https://p.superdesign.dev/draft/92941e0b-ae15-4b10-a794-2a77f6ccdfa5\" as=\"document\">\n    <link rel=\"prefetch\" href=\"https://p.superdesign.dev/draft/d2816504-2202-4bd3-b74c-c7760781c6f8\" as=\"document\">\n    <link rel=\"prefetch\" href=\"https://p.superdesign.dev/draft/dea53bf8-d572-4df4-b8df-f37140eef6c9\" as=\"document\">\n    <script src=\"https://cdn.tailwindcss.com\"></script>\n    <script src=\"https://code.iconify.design/iconify-icon/1.0.7/iconify-icon.min.js\"></script>\n    <link href=\"https://api.fontshare.com/v2/css?f[]=satoshi@900,700,500,400&display=swap\" rel=\"stylesheet\">\n    <style>\n        body {\n            font-family: 'Satoshi', sans-serif;\n            -webkit-font-smoothing: antialiased;\n        }\n        \n        .glass-panel {\n            background: rgba(255, 255, 255, 0.7);\n            backdrop-filter: blur(20px);\n            -webkit-backdrop-filter: blur(20px);\n            border: 1px solid rgba(255, 255, 255, 0.5);\n        }\n\n        .ios-input:focus-within {\n            border-color: #2563EB;\n            box-shadow: 0 0 0 4px rgba(37, 99, 235, 0.1);\n            transform: translateY(-1px);\n        }\n\n        @view-transition {\n            navigation: auto;\n        }\n\n        ::view-transition-old(main-content) {\n            animation: 0.25s ease-out both fade-out;\n        }\n\n        ::view-transition-new(main-content) {\n            animation: 0.25s ease-in 0.1s both fade-in;\n        }\n\n        @keyframes fade-out {\n            from { opacity: 1; }\n            to { opacity: 0; }\n        }\n\n        @keyframes fade-in {\n            from { opacity: 0; }\n            to { opacity: 1; }\n        }\n\n        ::-webkit-scrollbar {\n            width: 0px;\n            background: transparent;\n        }\n\n        .segmented-control-active {\n            background-color: white;\n            box-shadow: 0 2px 8px rgba(0,0,0,0.08);\n            color: #1e293b;\n        }\n    </style>\n</head>\n<body class=\"bg-[#F1F5F9]\">\n\n    <!-- Background Ambiance -->\n    <div class=\"fixed inset-0 overflow-hidden pointer-events-none z-0\">\n        <div class=\"absolute -top-[10%] left-[-10%] w-[120%] h-[60%] bg-gradient-to-b from-blue-100/50 via-teal-50/30 to-transparent blur-3xl\"></div>\n        <div class=\"absolute top-[10%] right-[-20%] w-[300px] h-[300px] bg-blue-400/10 rounded-full blur-[80px]\"></div>\n        <div class=\"absolute bottom-[20%] left-[-10%] w-[350px] h-[350px] bg-teal-300/10 rounded-full blur-[90px]\"></div>\n    </div>\n\n    <div id=\"root-container\" class=\"relative z-10 w-full min-h-screen flex flex-col pt-14 pb-[110px]\" style=\"view-transition-name: main-content;\">\n        \n        <div class=\"px-6\">\n            <!-- Header -->\n            <div class=\"flex justify-between items-center mb-6\">\n                <div>\n                    <h1 class=\"text-2xl font-bold text-slate-900 tracking-tight\">Vehicles</h1>\n                    <p class=\"text-slate-500 text-sm\">Manage your community access.</p>\n                </div>\n                <button onclick=\"toggleModal('add-vehicle-modal')\" class=\"w-10 h-10 rounded-full bg-blue-600 text-white flex items-center justify-center shadow-lg shadow-blue-600/20 active:scale-90 transition-transform\">\n                    <iconify-icon icon=\"lucide:plus\" class=\"text-xl\"></iconify-icon>\n                </button>\n            </div>\n\n            <!-- Segmented Control -->\n            <div class=\"p-1 bg-slate-200/50 rounded-xl flex mb-6\">\n                <button id=\"tab-my-vehicles\" onclick=\"switchTab('my-vehicles')\" class=\"flex-1 py-2 text-sm font-bold text-slate-500 rounded-lg transition-all segmented-control-active\">My Garage</button>\n                <button id=\"tab-history\" onclick=\"switchTab('history')\" class=\"flex-1 py-2 text-sm font-bold text-slate-500 rounded-lg transition-all\">Access Logs</button>\n            </div>\n\n            <!-- My Vehicles View -->\n            <div id=\"view-my-vehicles\" class=\"space-y-4\">\n                <!-- Vehicle Card 1 -->\n                <div class=\"glass-panel rounded-3xl p-5 shadow-sm border border-white/40 relative overflow-hidden\">\n                    <div class=\"absolute top-0 right-0 p-4\">\n                        <span class=\"px-3 py-1 bg-green-100 text-green-700 text-[10px] font-bold rounded-full uppercase tracking-wider\">Active Access</span>\n                    </div>\n                    <div class=\"flex items-center gap-4 mb-4\">\n                        <div class=\"w-14 h-14 bg-slate-100 rounded-2xl flex items-center justify-center\">\n                            <iconify-icon icon=\"lucide:car\" class=\"text-3xl text-slate-400\"></iconify-icon>\n                        </div>\n                        <div>\n                            <h3 class=\"text-lg font-bold text-slate-900\">Tesla Model 3</h3>\n                            <p class=\"text-slate-500 text-sm\">Midnight Silver • 2023</p>\n                        </div>\n                    </div>\n                    <div class=\"flex justify-between items-end border-t border-slate-100 pt-4\">\n                        <div>\n                            <p class=\"text-[10px] font-bold text-slate-400 uppercase tracking-widest\">License Plate</p>\n                            <p class=\"text-xl font-black text-slate-800 tracking-tighter\">ABC-1234</p>\n                        </div>\n                        <button class=\"flex items-center gap-1 text-blue-600 font-bold text-sm\">\n                            <span>Edit</span>\n                            <iconify-icon icon=\"lucide:chevron-right\"></iconify-icon>\n                        </button>\n                    </div>\n                </div>\n\n                <!-- Vehicle Card 2 -->\n                <div class=\"glass-panel rounded-3xl p-5 shadow-sm border border-white/40 relative\">\n                    <div class=\"absolute top-0 right-0 p-4\">\n                        <span class=\"px-3 py-1 bg-slate-100 text-slate-500 text-[10px] font-bold rounded-full uppercase tracking-wider\">Service Mode</span>\n                    </div>\n                    <div class=\"flex items-center gap-4 mb-4\">\n                        <div class=\"w-14 h-14 bg-slate-100 rounded-2xl flex items-center justify-center\">\n                            <iconify-icon icon=\"lucide:car\" class=\"text-3xl text-slate-400\"></iconify-icon>\n                        </div>\n                        <div>\n                            <h3 class=\"text-lg font-bold text-slate-900\">BMW X5</h3>\n                            <p class=\"text-slate-500 text-sm\">Alpine White • 2021</p>\n                        </div>\n                    </div>\n                    <div class=\"flex justify-between items-end border-t border-slate-100 pt-4\">\n                        <div>\n                            <p class=\"text-[10px] font-bold text-slate-400 uppercase tracking-widest\">License Plate</p>\n                            <p class=\"text-xl font-black text-slate-800 tracking-tighter\">BMW-9021</p>\n                        </div>\n                        <button class=\"flex items-center gap-1 text-blue-600 font-bold text-sm\">\n                            <span>Edit</span>\n                            <iconify-icon icon=\"lucide:chevron-right\"></iconify-icon>\n                        </button>\n                    </div>\n                </div>\n            </div>\n\n            <!-- Access History View -->\n            <div id=\"view-history\" class=\"hidden space-y-4\">\n                <div class=\"bg-white rounded-3xl border border-slate-100 divide-y divide-slate-50 overflow-hidden\">\n                    <div class=\"p-4 flex items-center gap-4\">\n                        <div class=\"w-10 h-10 bg-blue-50 text-blue-600 rounded-full flex items-center justify-center\">\n                            <iconify-icon icon=\"lucide:log-in\" class=\"text-xl\"></iconify-icon>\n                        </div>\n                        <div class=\"flex-1\">\n                            <h4 class=\"text-sm font-bold text-slate-800\">Entry - Main Gate</h4>\n                            <p class=\"text-xs text-slate-500\">Tesla Model 3 • ABC-1234</p>\n                        </div>\n                        <div class=\"text-right\">\n                            <p class=\"text-xs font-bold text-slate-900\">10:42 AM</p>\n                            <p class=\"text-[10px] text-slate-400\">Today</p>\n                        </div>\n                    </div>\n                    <div class=\"p-4 flex items-center gap-4\">\n                        <div class=\"w-10 h-10 bg-teal-50 text-teal-600 rounded-full flex items-center justify-center\">\n                            <iconify-icon icon=\"lucide:log-out\" class=\"text-xl\"></iconify-icon>\n                        </div>\n                        <div class=\"flex-1\">\n                            <h4 class=\"text-sm font-bold text-slate-800\">Exit - North Gate</h4>\n                            <p class=\"text-xs text-slate-500\">Tesla Model 3 • ABC-1234</p>\n                        </div>\n                        <div class=\"text-right\">\n                            <p class=\"text-xs font-bold text-slate-900\">08:15 AM</p>\n                            <p class=\"text-[10px] text-slate-400\">Today</p>\n                        </div>\n                    </div>\n                    <div class=\"p-4 flex items-center gap-4\">\n                        <div class=\"w-10 h-10 bg-blue-50 text-blue-600 rounded-full flex items-center justify-center\">\n                            <iconify-icon icon=\"lucide:log-in\" class=\"text-xl\"></iconify-icon>\n                        </div>\n                        <div class=\"flex-1\">\n                            <h4 class=\"text-sm font-bold text-slate-800\">Entry - Main Gate</h4>\n                            <p class=\"text-xs text-slate-500\">BMW X5 • BMW-9021</p>\n                        </div>\n                        <div class=\"text-right\">\n                            <p class=\"text-xs font-bold text-slate-900\">06:30 PM</p>\n                            <p class=\"text-[10px] text-slate-400\">Yesterday</p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n\n        <!-- Add Vehicle Modal (Overlay) -->\n        <div id=\"add-vehicle-modal\" class=\"fixed inset-0 z-[60] flex items-end justify-center hidden\">\n            <div class=\"absolute inset-0 bg-slate-900/40 backdrop-blur-sm transition-opacity duration-300\" onclick=\"toggleModal('add-vehicle-modal')\"></div>\n            <div class=\"relative w-full bg-white rounded-t-[40px] px-6 pt-8 pb-12 shadow-2xl transition-transform duration-500 translate-y-full\" id=\"modal-content\">\n                <div class=\"w-12 h-1.5 bg-slate-200 rounded-full mx-auto mb-8\"></div>\n                <h2 class=\"text-2xl font-bold text-slate-900 mb-6\">New Vehicle</h2>\n                \n                <form class=\"space-y-4\">\n                    <div class=\"grid grid-cols-2 gap-4\">\n                        <div class=\"space-y-1\">\n                            <label class=\"text-[10px] font-bold text-slate-400 uppercase tracking-widest ml-1\">Plate Number</label>\n                            <div class=\"ios-input flex items-center bg-slate-50 rounded-2xl border border-slate-200 px-4 h-14\">\n                                <input type=\"text\" placeholder=\"ABC-1234\" class=\"w-full bg-transparent outline-none text-slate-900 font-bold uppercase tracking-widest placeholder:normal-case placeholder:tracking-normal\">\n                            </div>\n                        </div>\n                        <div class=\"space-y-1\">\n                            <label class=\"text-[10px] font-bold text-slate-400 uppercase tracking-widest ml-1\">Year</label>\n                            <div class=\"ios-input flex items-center bg-slate-50 rounded-2xl border border-slate-200 px-4 h-14\">\n                                <input type=\"number\" placeholder=\"2024\" class=\"w-full bg-transparent outline-none text-slate-900 font-bold\">\n                            </div>\n                        </div>\n                    </div>\n\n                    <div class=\"space-y-1\">\n                        <label class=\"text-[10px] font-bold text-slate-400 uppercase tracking-widest ml-1\">Brand & Model</label>\n                        <div class=\"ios-input flex items-center bg-slate-50 rounded-2xl border border-slate-200 px-4 h-14\">\n                            <input type=\"text\" placeholder=\"e.g. Tesla Model Y\" class=\"w-full bg-transparent outline-none text-slate-900 font-bold\">\n                        </div>\n                    </div>\n\n                    <div class=\"space-y-1\">\n                        <label class=\"text-[10px] font-bold text-slate-400 uppercase tracking-widest ml-1\">Color</label>\n                        <div class=\"ios-input flex items-center bg-slate-50 rounded-2xl border border-slate-200 px-4 h-14\">\n                            <input type=\"text\" placeholder=\"Solid Black\" class=\"w-full bg-transparent outline-none text-slate-900 font-bold\">\n                        </div>\n                    </div>\n\n                    <button type=\"button\" onclick=\"toggleModal('add-vehicle-modal')\" class=\"w-full h-14 bg-blue-600 rounded-2xl text-white font-bold text-lg shadow-xl shadow-blue-500/20 active:scale-95 transition-all mt-4\">\n                        Add Vehicle\n                    </button>\n                </form>\n            </div>\n        </div>\n\n        <!-- Persistent Bottom Navigation -->\n        <div class=\"fixed bottom-0 left-0 right-0 glass-panel border-t border-slate-200/60 px-6 py-4 flex justify-between items-center z-50 rounded-t-[32px] shadow-[0_-10px_40px_rgba(0,0,0,0.05)]\">\n            <a id=\"nav-home-link\" href=\"https://p.superdesign.dev/draft/ddcad9de-e4e9-428d-9760-1a000213005c\" class=\"flex flex-col items-center gap-1 text-slate-400 hover:text-blue-500 transition-colors\">\n                <iconify-icon icon=\"lucide:home\" class=\"text-2xl\"></iconify-icon>\n                <span class=\"text-[10px] font-extrabold uppercase tracking-tighter\">Home</span>\n            </a>\n            <a id=\"nav-social-link\" href=\"https://p.superdesign.dev/draft/4b2e4ea0-eda0-4655-a976-ada345ba97ee\" class=\"flex flex-col items-center gap-1 text-slate-400 hover:text-blue-500 transition-colors\">\n                <iconify-icon icon=\"lucide:message-square\" class=\"text-2xl\"></iconify-icon>\n                <span class=\"text-[10px] font-extrabold uppercase tracking-tighter\">Social</span>\n            </a>\n            <a id=\"nav-billing-link\" href=\"https://p.superdesign.dev/draft/7a31d81c-233d-43bf-88f1-ce9c3f60c160\" class=\"flex flex-col items-center gap-1 text-slate-400 hover:text-blue-500 transition-colors\">\n                <iconify-icon icon=\"lucide:credit-card\" class=\"text-2xl\"></iconify-icon>\n                <span class=\"text-[10px] font-extrabold uppercase tracking-tighter\">Billing</span>\n            </a>\n            <a id=\"nav-profile-link\" href=\"https://p.superdesign.dev/draft/c51716d3-af56-4b12-b421-07490d1b5c93\" class=\"flex flex-col items-center gap-1 text-slate-400 hover:text-blue-500 transition-colors\">\n                <iconify-icon icon=\"lucide:user\" class=\"text-2xl\"></iconify-icon>\n                <span class=\"text-[10px] font-extrabold uppercase tracking-tighter\">Profile</span>\n            </a>\n        </div>\n\n    </div>\n\n    <script>\n        function switchTab(tab) {\n            const myVehicles = document.getElementById('view-my-vehicles');\n            const history = document.getElementById('view-history');\n            const tabVehicles = document.getElementById('tab-my-vehicles');\n            const tabHistory = document.getElementById('tab-history');\n\n            if (tab === 'my-vehicles') {\n                myVehicles.classList.remove('hidden');\n                history.classList.add('hidden');\n                tabVehicles.classList.add('segmented-control-active');\n                tabHistory.classList.remove('segmented-control-active');\n            } else {\n                myVehicles.classList.add('hidden');\n                history.classList.remove('hidden');\n                tabVehicles.classList.remove('segmented-control-active');\n                tabHistory.classList.add('segmented-control-active');\n            }\n        }\n\n        function toggleModal(id) {\n            const modal = document.getElementById(id);\n            const content = document.getElementById('modal-content');\n            \n            if (modal.classList.contains('hidden')) {\n                modal.classList.remove('hidden');\n                setTimeout(() => {\n                    content.classList.remove('translate-y-full');\n                }, 10);\n            } else {\n                content.classList.add('translate-y-full');\n                setTimeout(() => {\n                    modal.classList.add('hidden');\n                }, 500);\n            }\n        }\n    </script>\n</body>\n</html>"
}
